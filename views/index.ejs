<%- include('./partials/header')-%>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>Liste des constructeurs</h2>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                    </tr>
                </thead>
                <tbody>
                    <% factories.forEach(function(factory) {%>
                    <tr>
                        <th scope="row"><%=factory.id%></th>
                        <td><%=factory.name%></td>
                    </tr>
                    <%});%>
                </tbody>
            </table>
            <h2>Liste des Ã©nergies</h2>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                    </tr>
                </thead>
                <tbody>
                    <% energies.forEach(function(energy) {%>
                    <tr>
                        <th scope="row"><%=energy.id%></th>
                        <td><%=energy.name%></td>
                    </tr>
                    <%});%>
                </tbody>
            </table>
        </div>
    </div>
</div>

